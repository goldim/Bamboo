{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "fonts": {
        "local": true,
        "fontTypes": [
          "ttf"
        ]
      },
      "outputPath": "compiled/source",
      "bundle": {
        "include": [
          "qx.*"
        ]
      }
    },
    {
      "type": "build",
      "fonts": {
        "local": true,
        "fontTypes": [
          "ttf"
        ]
      },
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "environment": {
    "qx.icontheme": "Oxygen"
  },
  "applications": [
    {
      "class": "bamboo.demo.Application",
      "theme": "bamboo.theme.Theme",
      "name": "bamboo.demo",
      "title": "Bamboo Demo"
    }
  ]
}